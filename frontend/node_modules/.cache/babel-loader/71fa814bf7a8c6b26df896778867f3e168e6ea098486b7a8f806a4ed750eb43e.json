{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nimport countries from 'i18n-iso-countries';\nimport ja from 'i18n-iso-countries/langs/ja.json';\ncountries.registerLocale(ja);\nexport default {\n  name: 'UserEditView',\n  data() {\n    return {\n      form: {\n        username: '',\n        name: '',\n        age: null,\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: '',\n      countries: []\n    };\n  },\n  created() {\n    const countryObj = countries.getNames('ja');\n    this.countries = Object.values(countryObj).sort();\n    this.loadUser();\n  },\n  methods: {\n    formatPostalCode() {\n      let value = this.form.postalCode.replace(/[^\\d-]/g, '');\n      value = value.replace(/-/g, '');\n      if (value.length >= 4) {\n        value = value.slice(0, 3) + '-' + value.slice(3, 7);\n      }\n      this.form.postalCode = value;\n    },\n    formatPhoneNumber() {\n      let value = this.form.phoneNumber.replace(/[^\\d-]/g, '');\n      value = value.replace(/-/g, '');\n      if (value.length >= 11) {\n        value = value.slice(0, 3) + '-' + value.slice(3, 7) + '-' + value.slice(7, 11);\n      } else if (value.length >= 10) {\n        if (value.startsWith('0') && value[1] !== '0') {\n          if (['03', '04', '06'].includes(value.slice(0, 2))) {\n            value = value.slice(0, 2) + '-' + value.slice(2, 6) + '-' + value.slice(6, 10);\n          } else {\n            value = value.slice(0, 3) + '-' + value.slice(3, 6) + '-' + value.slice(6, 10);\n          }\n        }\n      } else if (value.length >= 7) {\n        value = value.slice(0, 3) + '-' + value.slice(3);\n      }\n      this.form.phoneNumber = value;\n    },\n    async loadUser() {\n      const userId = this.$route.params.id;\n      try {\n        const response = await fetch(`http://localhost:8081/api/users/${userId}`);\n        const data = await response.json();\n        this.form.username = data.username;\n        this.form.name = data.name;\n        this.form.age = data.age;\n        this.form.gender = data.gender;\n        this.form.postalCode = data.postalCode;\n        this.form.prefecture = data.prefecture;\n        this.form.city = data.city;\n        this.form.address = data.address;\n        this.form.phoneNumber = data.phoneNumber;\n        this.form.nationality = data.nationality;\n        this.favoriteFoodsInput = data.favoriteFoods.join(', ');\n      } catch (error) {\n        console.error('ユーザー情報の取得に失敗しました:', error);\n        this.errorMessage = 'ユーザー情報の取得に失敗しました';\n      }\n    },\n    async fetchAddress() {\n      const postalCode = this.form.postalCode.replace(/-/g, '');\n      if (postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${postalCode}`);\n          const data = await response.json();\n          if (data.results) {\n            this.form.prefecture = data.results[0].address1;\n            this.form.city = data.results[0].address2 + data.results[0].address3;\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error);\n        }\n      }\n    },\n    async handleUpdate() {\n      this.errorMessage = '';\n      this.successMessage = '';\n      const favoriteFoods = this.favoriteFoodsInput.split(',').map(food => food.trim()).filter(food => food !== '');\n      const userId = this.$route.params.id;\n      try {\n        const response = await fetch(`http://localhost:8081/api/users/${userId}`, {\n          method: 'PUT',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            ...this.form,\n            favoriteFoods: favoriteFoods\n          })\n        });\n        const data = await response.json();\n        if (response.ok) {\n          this.successMessage = '更新成功！一覧画面に戻ります...';\n          setTimeout(() => {\n            this.$router.push('/users');\n          }, 1500);\n        } else {\n          if (data.errors) {\n            const errorMessages = Object.values(data.errors).join('\\n');\n            this.errorMessage = errorMessages;\n          } else {\n            this.errorMessage = data.error || '更新に失敗しました';\n          }\n        }\n      } catch (error) {\n        console.error('更新エラー:', error);\n        this.errorMessage = '接続エラーが発生しました';\n      }\n    },\n    goToList() {\n      this.$router.push('/users');\n    }\n  }\n};","map":{"version":3,"names":["countries","ja","registerLocale","name","data","form","username","age","gender","postalCode","prefecture","city","address","phoneNumber","nationality","favoriteFoodsInput","errorMessage","successMessage","created","countryObj","getNames","Object","values","sort","loadUser","methods","formatPostalCode","value","replace","length","slice","formatPhoneNumber","startsWith","includes","userId","$route","params","id","response","fetch","json","favoriteFoods","join","error","console","fetchAddress","results","address1","address2","address3","handleUpdate","split","map","food","trim","filter","method","headers","body","JSON","stringify","ok","setTimeout","$router","push","errors","errorMessages","goToList"],"sources":["/Users/taharak_tabthinkmacbookpro/eclipse-workspace /tabthink-learning/frontend/src/views/UserEditView.vue"],"sourcesContent":["<template>\n  <div class=\"edit-container\">\n    <h1>ユーザー情報編集</h1>\n    <form @submit.prevent=\"handleUpdate\">\n      <div class=\"form-group\">\n        <label>ユーザー名（変更不可）</label>\n        <input v-model=\"form.username\" type=\"text\" disabled />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>氏名</label>\n        <input v-model=\"form.name\" type=\"text\" required />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>年齢</label>\n        <input v-model.number=\"form.age\" type=\"number\" min=\"0\" max=\"150\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>性別</label>\n        <div class=\"radio-group\">\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"男性\" /> 男性</label>\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"女性\" /> 女性</label>\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"その他\" /> その他</label>\n        </div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>郵便番号（ハイフンなし）</label>\n        <input \n          v-model=\"form.postalCode\" \n          type=\"text\" \n          @blur=\"fetchAddress\" \n          @input=\"formatPostalCode\"\n          placeholder=\"1000001\" \n          maxlength=\"8\"\n        />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>都道府県</label>\n        <input v-model=\"form.prefecture\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>市区町村</label>\n        <input v-model=\"form.city\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>市区町村以降</label>\n        <input v-model=\"form.address\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>電話番号</label>\n        <input \n          v-model=\"form.phoneNumber\" \n          type=\"tel\" \n          @input=\"formatPhoneNumber\"\n          placeholder=\"09012345678\"\n          maxlength=\"13\"\n        />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>国籍</label>\n        <select v-model=\"form.nationality\" required>\n          <option value=\"\">選択してください</option>\n          <option v-for=\"(country, index) in countries\" :key=\"index\" :value=\"country\">\n            {{ country }}\n          </option>\n        </select>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>好きな食べ物（カンマ区切りで複数入力可）</label>\n        <input v-model=\"favoriteFoodsInput\" type=\"text\" placeholder=\"例: ラーメン,寿司,カレー\" />\n      </div>\n      \n      <div v-if=\"errorMessage\" class=\"error\">{{ errorMessage }}</div>\n      <div v-if=\"successMessage\" class=\"success\">{{ successMessage }}</div>\n      \n      <button type=\"submit\" class=\"save-btn\">保存</button>\n      <button type=\"button\" class=\"back-btn\" @click=\"goToList\">戻る</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport countries from 'i18n-iso-countries'\nimport ja from 'i18n-iso-countries/langs/ja.json'\n\ncountries.registerLocale(ja)\n\nexport default {\n  name: 'UserEditView',\n  data() {\n    return {\n      form: {\n        username: '',\n        name: '',\n        age: null,\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: '',\n      countries: []\n    }\n  },\n  created() {\n    const countryObj = countries.getNames('ja')\n    this.countries = Object.values(countryObj).sort()\n    this.loadUser()\n  },\n  methods: {\n    formatPostalCode() {\n      let value = this.form.postalCode.replace(/[^\\d-]/g, '')\n      value = value.replace(/-/g, '')\n      \n      if (value.length >= 4) {\n        value = value.slice(0, 3) + '-' + value.slice(3, 7)\n      }\n      \n      this.form.postalCode = value\n    },\n    formatPhoneNumber() {\n      let value = this.form.phoneNumber.replace(/[^\\d-]/g, '')\n      value = value.replace(/-/g, '')\n      \n      if (value.length >= 11) {\n        value = value.slice(0, 3) + '-' + value.slice(3, 7) + '-' + value.slice(7, 11)\n      } else if (value.length >= 10) {\n        if (value.startsWith('0') && value[1] !== '0') {\n          if (['03', '04', '06'].includes(value.slice(0, 2))) {\n            value = value.slice(0, 2) + '-' + value.slice(2, 6) + '-' + value.slice(6, 10)\n          } else {\n            value = value.slice(0, 3) + '-' + value.slice(3, 6) + '-' + value.slice(6, 10)\n          }\n        }\n      } else if (value.length >= 7) {\n        value = value.slice(0, 3) + '-' + value.slice(3)\n      }\n      \n      this.form.phoneNumber = value\n    },\n    async loadUser() {\n      const userId = this.$route.params.id\n      try {\n        const response = await fetch(`http://localhost:8081/api/users/${userId}`)\n        const data = await response.json()\n        \n        this.form.username = data.username\n        this.form.name = data.name\n        this.form.age = data.age\n        this.form.gender = data.gender\n        this.form.postalCode = data.postalCode\n        this.form.prefecture = data.prefecture\n        this.form.city = data.city\n        this.form.address = data.address\n        this.form.phoneNumber = data.phoneNumber\n        this.form.nationality = data.nationality\n        this.favoriteFoodsInput = data.favoriteFoods.join(', ')\n      } catch (error) {\n        console.error('ユーザー情報の取得に失敗しました:', error)\n        this.errorMessage = 'ユーザー情報の取得に失敗しました'\n      }\n    },\n    async fetchAddress() {\n      const postalCode = this.form.postalCode.replace(/-/g, '')\n      \n      if (postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${postalCode}`)\n          const data = await response.json()\n          \n          if (data.results) {\n            this.form.prefecture = data.results[0].address1\n            this.form.city = data.results[0].address2 + data.results[0].address3\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error)\n        }\n      }\n    },\n    async handleUpdate() {\n      this.errorMessage = ''\n      this.successMessage = ''\n      \n      const favoriteFoods = this.favoriteFoodsInput\n        .split(',')\n        .map(food => food.trim())\n        .filter(food => food !== '')\n      \n      const userId = this.$route.params.id\n      \n      try {\n        const response = await fetch(`http://localhost:8081/api/users/${userId}`, {\n          method: 'PUT',\n          headers: { 'Content-Type': 'application/json' },\n          body: JSON.stringify({\n            ...this.form,\n            favoriteFoods: favoriteFoods\n          })\n        })\n        \n        const data = await response.json()\n        \n        if (response.ok) {\n          this.successMessage = '更新成功！一覧画面に戻ります...'\n          setTimeout(() => {\n            this.$router.push('/users')\n          }, 1500)\n        } else {\n          if (data.errors) {\n            const errorMessages = Object.values(data.errors).join('\\n')\n            this.errorMessage = errorMessages\n          } else {\n            this.errorMessage = data.error || '更新に失敗しました'\n          }\n        }\n      } catch (error) {\n        console.error('更新エラー:', error)\n        this.errorMessage = '接続エラーが発生しました'\n      }\n    },\n    goToList() {\n      this.$router.push('/users')\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* スタイルは変更なし */\n.edit-container {\n  max-width: 500px;\n  margin: 50px auto;\n  padding: 30px;\n  border: 1px solid #ddd;\n  border-radius: 8px;\n}\n\nh1 {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\nlabel {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: bold;\n}\n\ninput, select {\n  width: 100%;\n  padding: 10px;\n  font-size: 14px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  box-sizing: border-box;\n}\n\ninput:disabled {\n  background-color: #f0f0f0;\n  cursor: not-allowed;\n}\n\n.radio-group {\n  display: flex;\n  gap: 20px;\n}\n\n.radio-group label {\n  display: flex;\n  align-items: center;\n  font-weight: normal;\n}\n\n.radio-group input {\n  widt