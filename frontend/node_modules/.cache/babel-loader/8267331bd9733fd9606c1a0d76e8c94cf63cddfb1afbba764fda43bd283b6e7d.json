{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nexport default {\n  name: 'UserEditView',\n  data() {\n    return {\n      userId: null,\n      formData: {\n        username: '',\n        name: '',\n        gender: '',\n        age: null,\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      fieldErrors: {},\n      postalCodeNotFound: false,\n      countries: [] // ÂõΩÂêç„É™„Çπ„Éà\n    };\n  },\n  computed: {\n    hasErrors() {\n      return Object.keys(this.fieldErrors).length > 0 || this.postalCodeNotFound;\n    }\n  },\n  async mounted() {\n    this.userId = this.$route.params.id;\n    await this.loadCountries();\n    await this.loadUser();\n  },\n  methods: {\n    // ÂõΩÂêç„É™„Çπ„Éà„ÇíÂèñÂæó\n    async loadCountries() {\n      try {\n        const response = await fetch('https://restcountries.com/v3.1/all?fields=translations');\n        const data = await response.json();\n\n        // Êó•Êú¨Ë™ûÂêç„ÇíÊäΩÂá∫„Åó„Å¶„ÇΩ„Éº„Éà\n        this.countries = data.map(country => country.translations.jpn?.common || country.translations.jpn?.official).filter(name => name) // null/undefined„ÇíÈô§Â§ñ\n        .sort((a, b) => a.localeCompare(b, 'ja')); // Êó•Êú¨Ë™û„Åß„ÇΩ„Éº„Éà\n\n        // Êó•Êú¨„ÇíÂÖàÈ†≠„Å´\n        const japanIndex = this.countries.indexOf('Êó•Êú¨');\n        if (japanIndex > -1) {\n          this.countries.splice(japanIndex, 1);\n          this.countries.unshift('Êó•Êú¨');\n        }\n      } catch (error) {\n        console.error('ÂõΩÂêç„É™„Çπ„Éà„ÅÆÂèñÂæó„Å´Â§±Êïó„Åó„Åæ„Åó„Åü:', error);\n        // „Éï„Ç©„Éº„É´„Éê„ÉÉ„ÇØ: Âü∫Êú¨ÁöÑ„Å™ÂõΩÂêç„É™„Çπ„Éà\n        this.countries = ['Êó•Êú¨', '„Ç¢„É°„É™„Ç´ÂêàË°ÜÂõΩ', '‰∏≠ÂõΩ', 'ÈüìÂõΩ', 'Âè∞Êπæ', '„Ç§„ÇÆ„É™„Çπ', '„Éï„É©„É≥„Çπ', '„Éâ„Ç§„ÉÑ', '„Ç´„Éä„ÉÄ', '„Ç™„Éº„Çπ„Éà„É©„É™„Ç¢'];\n      }\n    },\n    async loadUser() {\n      try {\n        const response = await fetch(`http://localhost:8081/api/users/${this.userId}`);\n        if (!response.ok) {\n          throw new Error('„É¶„Éº„Ç∂„ÉºÊÉÖÂ†±„ÅÆÂèñÂæó„Å´Â§±Êïó„Åó„Åæ„Åó„Åü');\n        }\n        const user = await response.json();\n        this.formData = {\n          username: user.username,\n          name: user.name,\n          gender: user.gender === 'Áî∑' ? 'Áî∑ÊÄß' : user.gender === 'Â•≥' ? 'Â•≥ÊÄß' : user.gender,\n          age: user.age,\n          postalCode: user.postalCode ? user.postalCode.replace(/-/g, '') : '',\n          prefecture: user.prefecture,\n          city: user.city,\n          address: user.address,\n          phoneNumber: user.phoneNumber ? user.phoneNumber.replace(/-/g, '') : '',\n          nationality: user.nationality\n        };\n        this.favoriteFoodsInput = user.favoriteFoods ? user.favoriteFoods.join(',') : '';\n      } catch (error) {\n        console.error('„É¶„Éº„Ç∂„ÉºÊÉÖÂ†±„ÅÆÂèñÂæó„Å´Â§±Êïó„Åó„Åæ„Åó„Åü:', error);\n        alert('„É¶„Éº„Ç∂„ÉºÊÉÖÂ†±„ÅÆÂèñÂæó„Å´Â§±Êïó„Åó„Åæ„Åó„Åü');\n      }\n    },\n    clearFieldError(fieldName) {\n      if (this.fieldErrors[fieldName]) {\n        delete this.fieldErrors[fieldName];\n      }\n    },\n    formatPostalCodeNoHyphen() {\n      this.formData.postalCode = this.formData.postalCode.replace(/[^0-9]/g, '').substring(0, 7);\n      this.clearFieldError('postalCode');\n      this.postalCodeNotFound = false;\n    },\n    formatPhoneNumberNoHyphen() {\n      this.formData.phoneNumber = this.formData.phoneNumber.replace(/[^0-9]/g, '').substring(0, 11);\n      this.clearFieldError('phoneNumber');\n    },\n    async fetchAddress() {\n      if (this.formData.postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${this.formData.postalCode}`);\n          const data = await response.json();\n          if (data.results) {\n            const address = data.results[0];\n            this.formData.prefecture = address.address1;\n            this.formData.city = address.address2;\n            this.postalCodeNotFound = false;\n          } else {\n            this.formData.prefecture = '';\n            this.formData.city = '';\n            this.postalCodeNotFound = true;\n          }\n        } catch (error) {\n          console.error('‰ΩèÊâÄÂèñÂæó„Ç®„É©„Éº:', error);\n        }\n      }\n    },\n    async handleUpdate() {\n      if (this.postalCodeNotFound) {\n        this.scrollToTop();\n        return;\n      }\n      const favoriteFoodsArray = this.favoriteFoodsInput ? this.favoriteFoodsInput.split(',').map(item => item.trim()).filter(item => item) : [];\n      const requestData = {\n        name: this.formData.name,\n        gender: this.formData.gender,\n        age: this.formData.age,\n        postalCode: this.formData.postalCode,\n        prefecture: this.formData.prefecture,\n        city: this.formData.city,\n        address: this.formData.address,\n        phoneNumber: this.formData.phoneNumber,\n        nationality: this.formData.nationality,\n        favoriteFoods: favoriteFoodsArray\n      };\n      console.log('üì§ ÈÄÅ‰ø°„Éá„Éº„Çø:', requestData);\n      try {\n        const response = await fetch(`http://localhost:8081/api/users/${this.userId}`, {\n          method: 'PUT',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(requestData)\n        });\n        const data = await response.json();\n        console.log('‚úÖ „Çµ„Éº„Éê„ÉºÂøúÁ≠î:', data);\n        if (response.ok && data.success) {\n          alert('Êõ¥Êñ∞„ÅåÂÆå‰∫Ü„Åó„Åæ„Åó„Åü');\n          this.$router.push('/users');\n        } else if (data.errors) {\n          this.fieldErrors = data.errors;\n          this.scrollToTop();\n        } else if (data.message) {\n          alert(data.message);\n        }\n      } catch (error) {\n        console.error('‚ùå Êõ¥Êñ∞„Ç®„É©„Éº:', error);\n        alert('Êé•Á∂ö„Ç®„É©„Éº„ÅåÁô∫Áîü„Åó„Åæ„Åó„Åü');\n      }\n    },\n    scrollToTop() {\n      this.$nextTick(() => {\n        if (this.$refs.errorSummary) {\n          this.$refs.errorSummary.scrollIntoView({\n            behavior: 'smooth',\n            block: 'start'\n          });\n        } else {\n          window.scrollTo({\n            top: 0,\n            behavior: 'smooth'\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","userId","formData","username","gender","age","postalCode","prefecture","city","address","phoneNumber","nationality","favoriteFoodsInput","fieldErrors","postalCodeNotFound","countries","computed","hasErrors","Object","keys","length","mounted","$route","params","id","loadCountries","loadUser","methods","response","fetch","json","map","country","translations","jpn","common","official","filter","sort","a","b","localeCompare","japanIndex","indexOf","splice","unshift","error","console","ok","Error","user","replace","favoriteFoods","join","alert","clearFieldError","fieldName","formatPostalCodeNoHyphen","substring","formatPhoneNumberNoHyphen","fetchAddress","results","address1","address2","handleUpdate","scrollToTop","favoriteFoodsArray","split","item","trim","requestData","log","method","headers","body","JSON","stringify","success","$router","push","errors","message","$nextTick","$refs","errorSummary","scrollIntoView","behavior","block","window","scrollTo","top"],"sources":["/Users/taharak_tabthinkmacbookpro/eclipse-workspace /tabthink-learning/frontend/src/views/UserEditView.vue"],"sourcesContent":["<template>\n  <div class=\"edit-container\">\n    <h2>„É¶„Éº„Ç∂„ÉºÊÉÖÂ†±Á∑®ÈõÜ</h2>\n\n    <!-- „Ç®„É©„Éº„Çµ„Éû„É™„ÉºÔºà‰∏ÄÁï™‰∏äÔºâ -->\n    <div v-if=\"hasErrors\" class=\"error-summary\" ref=\"errorSummary\">\n      ‚ö†Ô∏è ÂÖ•ÂäõÂÜÖÂÆπ„Å´Ë™§„Çä„Åå„ÅÇ„Çä„Åæ„Åô\n    </div>\n\n    <form @submit.prevent=\"handleUpdate\" class=\"edit-form\">\n      <!-- „É¶„Éº„Ç∂„ÉºÂêçÔºàÁ∑®ÈõÜ‰∏çÂèØÔºâ -->\n      <div class=\"form-group\">\n        <label>„É¶„Éº„Ç∂„ÉºÂêç</label>\n        <input type=\"text\" v-model=\"formData.username\" disabled />\n      </div>\n\n      <!-- Ê∞èÂêç -->\n      <div class=\"form-group\">\n        <label>Ê∞èÂêç <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.name\"\n          :class=\"{ 'error-input': fieldErrors.name }\"\n          @input=\"clearFieldError('name')\"\n          placeholder=\"‰æã: Â±±Áî∞Â§™ÈÉé\"\n        />\n        <div v-if=\"fieldErrors.name\" class=\"error-message\">\n          {{ fieldErrors.name }}\n        </div>\n      </div>\n\n      <!-- ÊÄßÂà• -->\n      <div class=\"form-group\">\n        <label>ÊÄßÂà• <span class=\"required\">*</span></label>\n        <div :class=\"{ 'error-input': fieldErrors.gender }\">\n          <label class=\"radio-label\">\n            <input\n              type=\"radio\"\n              v-model=\"formData.gender\"\n              value=\"Áî∑ÊÄß\"\n              @change=\"clearFieldError('gender')\"\n            />\n            Áî∑ÊÄß\n          </label>\n          <label class=\"radio-label\">\n            <input\n              type=\"radio\"\n              v-model=\"formData.gender\"\n              value=\"Â•≥ÊÄß\"\n              @change=\"clearFieldError('gender')\"\n            />\n            Â•≥ÊÄß\n          </label>\n          <label class=\"radio-label\">\n            <input\n              type=\"radio\"\n              v-model=\"formData.gender\"\n              value=\"„Åù„ÅÆ‰ªñ\"\n              @change=\"clearFieldError('gender')\"\n            />\n            „Åù„ÅÆ‰ªñ\n          </label>\n        </div>\n        <div v-if=\"fieldErrors.gender\" class=\"error-message\">\n          {{ fieldErrors.gender }}\n        </div>\n      </div>\n\n      <!-- Âπ¥ÈΩ¢ -->\n      <div class=\"form-group\">\n        <label>Âπ¥ÈΩ¢ <span class=\"required\">*</span></label>\n        <input\n          type=\"number\"\n          v-model.number=\"formData.age\"\n          :class=\"{ 'error-input': fieldErrors.age }\"\n          @input=\"clearFieldError('age')\"\n          min=\"0\"\n          max=\"150\"\n          placeholder=\"‰æã: 25\"\n        />\n        <div v-if=\"fieldErrors.age\" class=\"error-message\">\n          {{ fieldErrors.age }}\n        </div>\n      </div>\n\n      <!-- ÈÉµ‰æøÁï™Âè∑ -->\n      <div class=\"form-group\">\n        <label>ÈÉµ‰æøÁï™Âè∑ <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.postalCode\"\n          :class=\"{ 'error-input': fieldErrors.postalCode || postalCodeNotFound }\"\n          @input=\"formatPostalCodeNoHyphen\"\n          @blur=\"fetchAddress\"\n          maxlength=\"7\"\n          placeholder=\"‰æã: 1000001Ôºà„Éè„Ç§„Éï„É≥„Å™„Åó7Ê°ÅÔºâ\"\n        />\n        <div v-if=\"fieldErrors.postalCode\" class=\"error-message\">\n          {{ fieldErrors.postalCode }}\n        </div>\n        <div v-if=\"postalCodeNotFound\" class=\"error-message\">\n          „Åì„ÅÆÈÉµ‰æøÁï™Âè∑„ÅØÂ≠òÂú®„Åó„Åæ„Åõ„Çì\n        </div>\n      </div>\n\n      <!-- ÈÉΩÈÅìÂ∫úÁúå -->\n      <div class=\"form-group\">\n        <label>ÈÉΩÈÅìÂ∫úÁúå <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.prefecture\"\n          :class=\"{ 'error-input': fieldErrors.prefecture }\"\n          @input=\"clearFieldError('prefecture')\"\n          placeholder=\"‰æã: Êù±‰∫¨ÈÉΩ\"\n        />\n        <div v-if=\"fieldErrors.prefecture\" class=\"error-message\">\n          {{ fieldErrors.prefecture }}\n        </div>\n      </div>\n\n      <!-- Â∏ÇÂå∫Áî∫Êùë -->\n      <div class=\"form-group\">\n        <label>Â∏ÇÂå∫Áî∫Êùë <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.city\"\n          :class=\"{ 'error-input': fieldErrors.city }\"\n          @input=\"clearFieldError('city')\"\n          placeholder=\"‰æã: ÂçÉ‰ª£Áî∞Âå∫\"\n        />\n        <div v-if=\"fieldErrors.city\" class=\"error-message\">\n          {{ fieldErrors.city }}\n        </div>\n      </div>\n\n      <!-- ‰ΩèÊâÄ -->\n      <div class=\"form-group\">\n        <label>‰ΩèÊâÄ <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.address\"\n          :class=\"{ 'error-input': fieldErrors.address }\"\n          @input=\"clearFieldError('address')\"\n          placeholder=\"‰æã: ÂçÉ‰ª£Áî∞1-1-1\"\n        />\n        <div v-if=\"fieldErrors.address\" class=\"error-message\">\n          {{ fieldErrors.address }}\n        </div>\n      </div>\n\n      <!-- ÈõªË©±Áï™Âè∑ -->\n      <div class=\"form-group\">\n        <label>ÈõªË©±Áï™Âè∑ <span class=\"required\">*</span></label>\n        <input\n          type=\"tel\"\n          v-model=\"formData.phoneNumber\"\n          :class=\"{ 'error-input': fieldErrors.phoneNumber }\"\n          @input=\"formatPhoneNumberNoHyphen\"\n          maxlength=\"11\"\n          placeholder=\"‰æã: 09012345678Ôºà„Éè„Ç§„Éï„É≥„Å™„Åó10„Äú11Ê°ÅÔºâ\"\n        />\n        <div v-if=\"fieldErrors.phoneNumber\" class=\"error-message\">\n          {{ fieldErrors.phoneNumber }}\n        </div>\n      </div>\n\n      <!-- ÂõΩÁ±ç -->\n      <div class=\"form-group\">\n        <label>ÂõΩÁ±ç <span class=\"required\">*</span></label>\n        <select\n          v-model=\"formData.nationality\"\n          :class=\"{ 'error-input': fieldErrors.nationality }\"\n          @change=\"clearFieldError('nationality')\"\n        >\n        <option value=\"\">ÈÅ∏Êäû„Åó„Å¶„Åè„Å†„Åï„ÅÑ</option>\n        <option v-for=\"country in countries\" :key=\"country\" :value=\"country\">\n          {{ country }}\n        </option>\n        </select>\n        <div v-if=\"fieldErrors.nationality\" class=\"error-message\">\n          {{ fieldErrors.nationality }}\n      </div>\n     </div>\n\n      <!-- Â•Ω„Åç„Å™È£ü„ÅπÁâ© -->\n      <div class=\"form-group\">\n        <label>Â•Ω„Åç„Å™È£ü„ÅπÁâ©</label>\n        <input\n          type=\"text\"\n          v-model=\"favoriteFoodsInput\"\n          :class=\"{ 'error-input': fieldErrors.favoriteFoods }\"\n          @input=\"clearFieldError('favoriteFoods')\"\n          placeholder=\"‰æã: ÂØøÂè∏,„É©„Éº„É°„É≥,„Ç´„É¨„ÉºÔºà„Ç´„É≥„ÉûÂå∫Âàá„ÇäÔºâ\"\n        />\n        <div v-if=\"fieldErrors.favoriteFoods\" class=\"error-message\">\n          {{ fieldErrors.favoriteFoods }}\n        </div>\n      </div>\n\n      <!-- „Éú„Çø„É≥ -->\n      <div class=\"button-group\">\n        <button type=\"submit\" class=\"btn-primary\">Êõ¥Êñ∞</button>\n        <button type=\"button\" @click=\"$router.push('/users')\" class=\"btn-secondary\">\n          „Ç≠„É£„É≥„Çª„É´\n        </button>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'UserEditView',\n  data() {\n    return {\n      userId: null,\n      formData: {\n        username: '',\n        name: '',\n        gender: '',\n        age: null,\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: '',\n      },\n      favoriteFoodsInput: '',\n      fieldErrors: {},\n      postalCodeNotFound: false,\n      countries: [], // ÂõΩÂêç„É™„Çπ„Éà\n    };\n  },\n  computed: {\n    hasErrors() {\n      return Object.keys(this.fieldErrors).length > 0 || this.postalCodeNotFound;\n    }\n  },\n  async mounted() {\n    this.userId = this.$route.params.id;\n    await this.loadCountries();\n    await this.loadUser();\n  },\n  methods: {\n    // ÂõΩÂêç„É™„Çπ„Éà„ÇíÂèñÂæó\n    async loadCountries() {\n      try {\n        const response = await fetch('https://restcountries.com/v3.1/all?fields=translations');\n        const data = await response.json();\n        \n        // Êó•Êú¨Ë™ûÂêç„ÇíÊäΩÂá∫„Åó„Å¶„ÇΩ„Éº„Éà\n        this.countri