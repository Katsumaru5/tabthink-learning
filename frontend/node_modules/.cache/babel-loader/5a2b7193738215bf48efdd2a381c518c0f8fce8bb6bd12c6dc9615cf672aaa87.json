{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nimport countries from 'i18n-iso-countries';\nimport ja from 'i18n-iso-countries/langs/ja.json';\ncountries.registerLocale(ja);\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        name: '',\n        age: null,\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: '',\n      fieldErrors: {},\n      countries: []\n    };\n  },\n  created() {\n    const countryObj = countries.getNames('ja');\n    this.countries = Object.values(countryObj).sort();\n  },\n  methods: {\n    formatPostalCode() {\n      let value = this.form.postalCode.replace(/[^\\d-]/g, '');\n      value = value.replace(/-/g, '');\n      if (value.length >= 4) {\n        value = value.slice(0, 3) + '-' + value.slice(3, 7);\n      }\n      this.form.postalCode = value;\n\n      // エラーをクリア\n      if (this.fieldErrors.postalCode) {\n        delete this.fieldErrors.postalCode;\n      }\n    },\n    formatPhoneNumber() {\n      let value = this.form.phoneNumber.replace(/[^\\d-]/g, '');\n      value = value.replace(/-/g, '');\n      if (value.length >= 11) {\n        value = value.slice(0, 3) + '-' + value.slice(3, 7) + '-' + value.slice(7, 11);\n      } else if (value.length >= 10) {\n        if (value.startsWith('0') && value[1] !== '0') {\n          if (['03', '04', '06'].includes(value.slice(0, 2))) {\n            value = value.slice(0, 2) + '-' + value.slice(2, 6) + '-' + value.slice(6, 10);\n          } else {\n            value = value.slice(0, 3) + '-' + value.slice(3, 6) + '-' + value.slice(6, 10);\n          }\n        }\n      } else if (value.length >= 7) {\n        value = value.slice(0, 3) + '-' + value.slice(3);\n      }\n      this.form.phoneNumber = value;\n\n      // エラーをクリア\n      if (this.fieldErrors.phoneNumber) {\n        delete this.fieldErrors.phoneNumber;\n      }\n    },\n    async fetchAddress() {\n      const postalCode = this.form.postalCode.replace(/-/g, '');\n      if (postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${postalCode}`);\n          const data = await response.json();\n          if (data.results) {\n            this.form.prefecture = data.results[0].address1;\n            this.form.city = data.results[0].address2 + data.results[0].address3;\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error);\n        }\n      }\n    },\n    getFieldNameInJapanese(fieldName) {\n      const fieldMap = {\n        username: 'ユーザー名',\n        password: 'パスワード',\n        name: '氏名',\n        age: '年齢',\n        gender: '性別',\n        postalCode: '郵便番号',\n        prefecture: '都道府県',\n        city: '市区町村',\n        address: '市区町村以降の住所',\n        phoneNumber: '電話番号',\n        nationality: '国籍',\n        favoriteFoods: '好きな食べ物'\n      };\n      return fieldMap[fieldName] || fieldName;\n    },\n    async handleRegister() {\n      this.errorMessage = '';\n      this.successMessage = '';\n      this.fieldErrors = {};\n      const favoriteFoods = this.favoriteFoodsInput.split(',').map(food => food.trim()).filter(food => food !== '');\n      const requestData = {\n        ...this.form,\n        favoriteFoods: favoriteFoods\n      };\n      try {\n        const response = await fetch('http://localhost:8081/api/users/register', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(requestData)\n        });\n        const data = await response.json();\n        if (response.ok) {\n          this.successMessage = '✅ 登録成功！ログイン画面に移動します...';\n          setTimeout(() => {\n            this.$router.push('/login');\n          }, 2000);\n        } else {\n          if (data.errors) {\n            // フィールドごとのエラーを設定\n            this.fieldErrors = {};\n            const errorList = [];\n            for (const [field, message] of Object.entries(data.errors)) {\n              const japaneseFieldName = this.getFieldNameInJapanese(field);\n              this.fieldErrors[field] = message;\n              errorList.push(`・${japaneseFieldName}: ${message}`);\n            }\n            this.errorMessage = '以下の項目を修正してください:\\n' + errorList.join('\\n');\n          } else {\n            this.errorMessage = data.error || '登録に失敗しました';\n          }\n\n          // エラーがある最初のフィールドまでスクロール\n          this.$nextTick(() => {\n            const firstError = document.querySelector('.error-input');\n            if (firstError) {\n              firstError.scrollIntoView({\n                behavior: 'smooth',\n                block: 'center'\n              });\n              firstError.focus();\n            }\n          });\n        }\n      } catch (error) {\n        console.error('登録エラー:', error);\n        this.errorMessage = '❌ サーバーとの接続に失敗しました。ネットワーク接続を確認してください。';\n      }\n    },\n    goToLogin() {\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["countries","ja","registerLocale","name","data","form","username","password","age","gender","postalCode","prefecture","city","address","phoneNumber","nationality","favoriteFoodsInput","errorMessage","successMessage","fieldErrors","created","countryObj","getNames","Object","values","sort","methods","formatPostalCode","value","replace","length","slice","formatPhoneNumber","startsWith","includes","fetchAddress","response","fetch","json","results","address1","address2","address3","error","console","getFieldNameInJapanese","fieldName","field