{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nimport countries from 'i18n-iso-countries';\nimport ja from 'i18n-iso-countries/langs/ja.json';\ncountries.registerLocale(ja);\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        name: '',\n        age: null,\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: '',\n      fieldErrors: {},\n      postalCodeNotFound: false,\n      countries: []\n    };\n  },\n  created() {\n    const countryObj = countries.getNames('ja');\n    this.countries = Object.values(countryObj).sort();\n  },\n  methods: {\n    formatPostalCodeNoHyphen() {\n      // 数字以外を削除（ハイフンも削除）\n      let value = this.form.postalCode.replace(/[^\\d]/g, '');\n\n      // 7桁まで制限\n      if (value.length > 7) {\n        value = value.slice(0, 7);\n      }\n      this.form.postalCode = value;\n\n      // エラーをクリア\n      if (this.fieldErrors.postalCode) {\n        delete this.fieldErrors.postalCode;\n      }\n\n      // 郵便番号が存在しないエラーもクリア\n      this.postalCodeNotFound = false;\n    },\n    formatPhoneNumberNoHyphen() {\n      // 数字以外を削除（ハイフンも削除）\n      let value = this.form.phoneNumber.replace(/[^\\d]/g, '');\n\n      // 11桁まで制限\n      if (value.length > 11) {\n        value = value.slice(0, 11);\n      }\n      this.form.phoneNumber = value;\n      if (this.fieldErrors.phoneNumber) {\n        delete this.fieldErrors.phoneNumber;\n      }\n    },\n    async fetchAddress() {\n      const postalCode = this.form.postalCode.replace(/-/g, '');\n\n      // 郵便番号が存在しないエラーをリセット\n      this.postalCodeNotFound = false;\n      if (postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${postalCode}`);\n          const data = await response.json();\n          if (data.results) {\n            // 郵便番号が存在する場合\n            this.form.prefecture = data.results[0].address1;\n            this.form.city = data.results[0].address2 + data.results[0].address3;\n            this.postalCodeNotFound = false;\n          } else {\n            // 郵便番号が存在しない場合\n            this.postalCodeNotFound = true;\n            this.form.prefecture = '';\n            this.form.city = '';\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error);\n          this.postalCodeNotFound = true;\n        }\n      }\n    },\n    getFieldNameInJapanese(fieldName) {\n      const fieldMap = {\n        username: 'ユーザー名',\n        password: 'パスワード',\n        name: '氏名',\n        age: '年齢',\n        gender: '性別',\n        postalCode: '郵便番号',\n        prefecture: '都道府県',\n        city: '市区町村',\n        address: '市区町村以降の住所',\n        phoneNumber: '電話番号',\n        nationality: '国籍',\n        favoriteFoods: '好きな食べ物'\n      };\n      return fieldMap[fieldName] || fieldName;\n    },\n    async handleRegister() {\n      this.errorMessage = '';\n      this.successMessage = '';\n      this.fieldErrors = {};\n\n      // 郵便番号の存在チェック\n      if (this.postalCodeNotFound) {\n        this.errorMessage = '郵便番号が存在しません。正しい郵便番号を入力してください。';\n        return;\n      }\n      const favoriteFoods = this.favoriteFoodsInput.split(',').map(food => food.trim()).filter(food => food !== '');\n      const requestData = {\n        ...this.form,\n        favoriteFoods: favoriteFoods\n      };\n      console.log('========== 送信データ（登録） ==========');\n      console.log('全データ:', JSON.stringify(requestData, null, 2));\n      console.log('=======================================');\n      try {\n        const response = await fetch('http://localhost:8081/api/users/register', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(requestData)\n        });\n        const data = await response.json();\n        console.log('========== サーバーからの応答（登録） ==========');\n        console.log('ステータス:', response.status);\n        console.log('レスポンス:', JSON.stringify(data, null, 2));\n        console.log('===============================================');\n        if (response.ok) {\n          this.successMessage = '✅ 登録成功！ログイン画面に移動します...';\n          setTimeout(() => {\n            this.$router.push('/login');\n          }, 2000);\n        } else {\n          // エラーメッセージの処理\n          if (data.errors) {\n            this.fieldErrors = {};\n            const errorList = [];\n\n            // Spring Bootのエラー形式を処理\n            if (Array.isArray(data.errors)) {\n              // 配列形式のエラー\n              data.errors.forEach(error => {\n                if (error.field && error.defaultMessage) {\n                  const japaneseFieldName = this.getFieldNameInJapanese(error.field);\n                  this.fieldErrors[error.field] = error.defaultMessage;\n                  errorList.push(`・${japaneseFieldName}: ${error.defaultMessage}`);\n                } else if (typeof error === 'string') {\n                  errorList.push(`・${error}`);\n                }\n              });\n            } else if (typeof data.errors === 'object') {\n              // オブジェクト形式のエラー\n              for (const [field, message] of Object.entries(data.errors)) {\n                if (typeof message === 'string') {\n                  const japaneseFieldName = this.getFieldNameInJapanese(field);\n                  this.fieldErrors[field] = message;\n                  errorList.push(`・${japaneseFieldName}: ${message}`);\n                }\n              }\n            }\n            if (errorList.length > 0) {\n              this.errorMessage = '以下の項目を修正してください:\\n' + errorList.join('\\n');\n            } else {\n              this.errorMessage = 'バリデーションエラーが発生しました。入力内容を確認してください。';\n            }\n          } else if (data.message) {\n            this.errorMessage = data.message;\n          } else if (data.error) {\n            this.errorMessage = data.error;\n          } else {\n            this.errorMessage = '登録に失敗しました。入力内容を確認してください。';\n          }\n\n          // エラーがある最初のフィールドまでスクロール\n          this.$nextTick(() => {\n            const firstError = document.querySelector('.error-input');\n            if (firstError) {\n              firstError.scrollIntoView({\n                behavior: 'smooth',\n                block: 'center'\n              });\n              firstError.focus();\n            }\n          });\n        }\n      } catch (error) {\n        console.error('登録エラー:', error);\n        this.errorMessage = '❌ サーバーとの接続に失敗しました。ネットワーク接続を確認してください。';\n      }\n    },\n    goToLogin() {\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["countries","ja","registerLocale","name","data","form","username","password","age","gender","postalCode","prefecture","city","address","phoneNumber","nationality","favoriteFoodsInput","errorMessage","successMessage","fieldErrors","postalCodeNotFound","created","countryObj","getNames","Object","values","sort","methods","formatPostalCodeNoHyphen","value","replace","length","slice","formatPhoneNumberNoHyphen","fetchAddress","response","fetch","json","results","address1","address2","address3","error","console","getFieldNameInJapanese","fieldName","fieldMap","favoriteFoods","handleRegister","split","map","food","trim","filter","requestData","log","JSON","stringify","method","headers","body","status","ok","setTimeout","$router","push","errors","errorList","Array","isArray","forEach","field","defaultMessage","japaneseFieldName","message","entries","join","$nextTick","firstError","document","querySelector","scrollIntoView","behavior","block","focus","goToLogin"],"sources":["/Users/taharak_tabthinkmacbookpro/eclipse-workspace /tabthink-learning/frontend/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div class=\"register-container\">\n    <h1>ユーザー登録</h1>\n    <form @submit.prevent=\"handleRegister\">\n      <div class=\"form-group\">\n        <label>ユーザー名</label>\n        <input \n          v-model=\"form.username\" \n          type=\"text\" \n          required \n          :class=\"{ 'error-input': fieldErrors.username }\"\n        />\n        <div v-if=\"fieldErrors.username\" class=\"field-error\">{{ fieldErrors.username }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>パスワード</label>\n        <input \n          v-model=\"form.password\" \n          type=\"password\" \n          required \n          :class=\"{ 'error-input': fieldErrors.password }\"\n        />\n        <div v-if=\"fieldErrors.password\" class=\"field-error\">{{ fieldErrors.password }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>氏名</label>\n        <input \n          v-model=\"form.name\" \n          type=\"text\" \n          required \n          :class=\"{ 'error-input': fieldErrors.name }\"\n        />\n        <div v-if=\"fieldErrors.name\" class=\"field-error\">{{ fieldErrors.name }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>年齢</label>\n        <input \n          v-model.number=\"form.age\" \n          type=\"number\" \n          min=\"0\" \n          max=\"150\" \n          :class=\"{ 'error-input': fieldErrors.age }\"\n        />\n        <div v-if=\"fieldErrors.age\" class=\"field-error\">{{ fieldErrors.age }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>性別</label>\n        <div class=\"radio-group\" :class=\"{ 'error-border': fieldErrors.gender }\">\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"男性\" /> 男性</label>\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"女性\" /> 女性</label>\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"その他\" /> その他</label>\n        </div>\n        <div v-if=\"fieldErrors.gender\" class=\"field-error\">{{ fieldErrors.gender }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>郵便番号（ハイフンなし）</label>\n        <input \n          v-model=\"form.postalCode\" \n          type=\"text\" \n          @blur=\"fetchAddress\" \n          @input=\"formatPostalCodeNoHyphen\"\n          placeholder=\"1000001\" \n          maxlength=\"7\"\n          :class=\"{ 'error-input': fieldErrors.postalCode }\"\n        />\n        <div class=\"input-hint\">例: 1000001（7桁、ハイフンなし）</div>\n        <div v-if=\"fieldErrors.postalCode\" class=\"field-error\">{{ fieldErrors.postalCode }}</div>\n        <div v-if=\"postalCodeNotFound\" class=\"field-error\">この郵便番号は存在しません</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>都道府県</label>\n        <input \n          v-model=\"form.prefecture\" \n          type=\"text\" \n          :class=\"{ 'error-input': fieldErrors.prefecture }\"\n        />\n        <div v-if=\"fieldErrors.prefecture\" class=\"field-error\">{{ fieldErrors.prefecture }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>市区町村</label>\n        <input \n          v-model=\"form.city\" \n          type=\"text\" \n          :class=\"{ 'error-input': fieldErrors.city }\"\n        />\n        <div v-if=\"fieldErrors.city\" class=\"field-error\">{{ fieldErrors.city }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>市区町村以降</label>\n        <input \n          v-model=\"form.address\" \n          type=\"text\" \n          :class=\"{ 'error-input': fieldErrors.address }\"\n        />\n        <div v-if=\"fieldErrors.address\" class=\"field-error\">{{ fieldErrors.address }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>電話番号（ハイフンなし）</label>\n        <input \n          v-model=\"form.phoneNumber\" \n          type=\"tel\" \n          @input=\"formatPhoneNumberNoHyphen\"\n          placeholder=\"09012345678\"\n          maxlength=\"11\"\n          :class=\"{ 'error-input': fieldErrors.phoneNumber }\"\n        />\n        <div class=\"input-hint\">例: 09012345678（10桁または11桁、ハイフンなし）</div>\n        <div v-if=\"fieldErrors.phoneNumber\" class=\"field-error\">{{ fieldErrors.phoneNumber }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>国籍</label>\n        <select \n          v-model=\"form.nationality\" \n          required\n          :class=\"{ 'error-input': fieldErrors.nationality }\"\n        >\n          <option value=\"\">選択してください</option>\n          <option v-for=\"(country, index) in countries\" :key=\"index\" :value=\"country\">\n            {{ country }}\n          </option>\n        </select>\n        <div v-if=\"fieldErrors.nationality\" class=\"field-error\">{{ fieldErrors.nationality }}</div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>好きな食べ物（カンマ区切りで複数入力可）</label>\n        <input \n          v-model=\"favoriteFoodsInput\" \n          type=\"text\" \n          placeholder=\"例: ラーメン,寿司,カレー\"\n          :class=\"{ 'error-input': fieldErrors.favoriteFoods }\"\n        />\n        <div v-if=\"fieldErrors.favoriteFoods\" class=\"field-error\">{{ fieldErrors.favoriteFoods }}</div>\n      </div>\n      \n      <div v-if=\"errorMessage\" class=\"error-summary\">\n        <strong>⚠️ 入力内容に誤りがあります</strong>\n        <div class=\"error-details\">{{ errorMessage }}</div>\n      </div>\n      \n      <div v-if=\"successMessage\" class=\"success\">{{ successMessage }}</div>\n      \n      <button type=\"submit\" class=\"register-btn\">登録</button>\n      <button type=\"button\" class=\"back-btn\" @click=\"goToLogin\">戻る</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport countries from 'i18n-iso-countries'\nimport ja from 'i18n-iso-countries/langs/ja.json'\n\ncountries.registerLocale(ja)\n\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        name: '',\n        age: null,\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: '',\n      fieldErrors: {},\n      postalCodeNotFound: false,\n      countries: []\n    }\n  },\n  created() {\n    const countryObj = countries.getNames('ja')\n    this.countries = Object.values(countryObj).sort()\n  },\n  methods: {\n    formatPostalCodeNoHyphen() {\n      // 数字以外を削除（ハイフンも削除）\n      let value = this.form.postalCode.replace(/[^\\d]/g, '')\n      \n      // 7桁まで制限\n      if (value.length > 7) {\n        value = value.slice(0, 7)\n      }\n      \n      this.form.postalCode = value\n      \n      // エラーをクリア\n      if (this.fieldErrors.postalCode) {\n        delete this.fieldErrors.postalCode\n      }\n      \n      // 郵便番号が存在しないエラーもクリア\n      this.postalCodeNotFound = false\n    },\n    formatPhoneNumberNoHyphen() {\n      // 数字以外を削除（ハイフンも削除）\n      let value = this.form.phoneNumber.replace(/[^\\d]/g, '')\n      \n      // 11桁まで制限\n      if (value.length > 11) {\n        value = value.slice(0, 11)\n      }\n      \n      this.form.phoneNumber = value\n      \n      if (this.fieldErrors.phoneNumber) {\n        delete this.fieldErrors.phoneNumber\n      }\n    },\n    async fetchAddress() {\n      const postalCode = this.form.postalCode.replace(/-/g, '')\n      \n      // 郵便番号が存在しないエラーをリセット\n      this.postalCodeNotFound = false\n      \n      if (postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${postalCode}`)\n          const data = await response.json()\n          \n          if (data.results) {\n            // 郵便番号が存在する場合\n            this.form.prefecture = data.results[0].address1\n            this.form.city = data.results[0].address2 + data.results[0].address3\n            this.postalCodeNotFound = false\n          } else {\n            // 郵便番号が存在しない場合\n            this.postalCodeNotFound = true\n            this.form.prefecture = ''\n            this.form.city = ''\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error)\n          this.postalCodeNotFound = true\n        }\n      }\n    },\n    getFieldNameInJapanese(fieldName) {\n      const fieldMap = {\n        username: 'ユーザー名',\n        password: 'パスワード',\n        name: '氏名',\n        age: '年齢',\n        gender: '性別',\n        postalCode: '郵便番号',\n        prefecture: '都道府県',\n        city: '市区町村',\n        address: '市区町村以降の住所',\n        phoneNumber: '電話番号',\n        nationality: '国籍',\n        favoriteFoods: '好きな食べ物'\n      }\n      return fieldMap[fieldName] || fieldName\n    },\n    async handleRegister() {\n      this.errorMessage = ''\n      this.successMessage = ''\n      this.fieldErrors = {}\n      \n      // 郵便番号の存在チェック\n      if (this.postalCodeNotFound) {\n        this.errorMessage = '郵便番号が存在しません。正しい郵便番号を入力してください。'\n        return\n      }\n      \n      const favoriteFoods = this.favoriteFoodsInput\n        .split(',')\n        .map(food => food.trim())\n        .filter(food => food !== '')\n      \n      const requestData = {\n        ...this.form,\n        favoriteFoods: favoriteFoods\n      }\n      \n      console.log('========== 送信データ（登録） ==========')\n      console.log('全データ:', JSON.stringify(requestData, null, 2))\n      console.log('=======================================')\n      \n      try {\n        const response = await fetch('http://localhost:8081/api/users/register', {\n          method: 'POST',\n          headers: { 'Content-Type': 'application/json' },\n          body: JSON.stringify(requestData)\n        })\n        \n        const data = await response.json()\n        \n        console.log('========== サーバーからの応答（登録） ==========')\n        console.log('ステータス:', response.status)\n        console.log('レスポンス:', JSON.stringify(data, null, 2))\n        console.log('===============================================')\n        \n        if (response.ok) {\n          this.successMessage = '✅ 登録成功！ログイン画面に移動します...'\n          setTimeout(() => {\n            this.$router.push('/login')\n          }, 2000)\n        } else {\n          // エラーメッセージの処理\n          if (data.errors) {\n            this.fieldErrors = {}\n            const errorList = []\n            \n            // Spring Bootのエラー形式を処理\n            if (Array.isArray(data.errors)) {\n              // 配列形式のエラー\n              data.errors.forEach(error => {\n                if (error.field && error.defaultMessage) {\n                  const japaneseFieldName = this.getFieldNameInJapanese(error.field)\n                  this.fieldErrors[error.field] = error.defaultMessage\n                  errorList.push(`・${japaneseFieldName}: ${error.defaultMessage}`)\n                } else if (typeof error === 'string') {\n                  errorList.push(`・${error}`)\n                }\n              })\n            } else if (typeof data.errors === 'object') {\n              // オブジェクト形式のエラー\n              for (const [field, message] of Object.entries(data.errors)) {\n                if (typeof message === 'string') {\n                  const japaneseFieldName = this.getFieldNameInJapanese(field)\n                  this.fieldErrors[field] = message\n                  errorList.push(`・${japaneseFieldName}: ${message}`)\n                }\n              }\n            }\n            \n            if (errorList.length > 0) {\n              this.errorMessage = '以下の項目を修正してください:\\n' + errorList.join('\\n')\n            } else {\n              this.errorMessage = 'バリデーションエラーが発生しました。入力内容を確認してください。'\n            }\n          } else if (data.message) {\n            this.errorMessage = data.message\n          } else if (data.error) {\n            this.errorMessage = data.error\n          } else {\n            this.errorMessage = '登録に失敗しました。入力内容を確認してください。'\n          }\n          \n          // エラーがある最初のフィールドまでスクロール\n          this.$nextTick(() => {\n            const firstError = document.querySelector('.error-input')\n            if (firstError) {\n              firstError.scrollIntoView({ behavior: 'smooth', block: 'center' })\n              firstError.focus()\n            }\n          })\n        }\n      } catch (error) {\n        console.error('登録エラー:', error)\n        this.errorMessage = '❌ サーバーとの接続に失敗しました。ネットワーク接続を確認してください。'\n      }\n    },\n    goToLogin() {\n      this.$router.push('/login')\n    }\n  }\n}\n</script>\n\n<style scoped>\n.register-container {\n  max-width: 500px;\n  margin: 50px auto;\n  padding: 30px;\n  border: 1px solid #ddd;\n  border-radius: 8px;\n}\n\nh1 {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\nlabel {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: bold;\n}\n\ninput, select {\n  width: 100%;\n  padding: 10px;\n  font-size: 14px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  box-sizing: border-box;\n  transition: border-color 0.3s;\n}\n\ninput:focus, select:focus {\n  outline: none;\n  border-color: #4CAF50;\n}\n\n.error-input {\n  border-color: #f44336 !important;\n  background-color: #ffebee;\n}\n\n.error-border {\n  border: 2px solid #f44336;\n  padding: 10px;\n  border-radius: 4px;\n  background-color: #ffebee;\n}\n\n.field-error {\n  color: #f44336;\n  font-size: 12px;\n  margin-top: 5px;\n  display: flex;\n  align-items: center;\n}\n\n.field-error::before {\n  content: \"⚠️ \";\n  margin-right: 4px;\n}\n\n.input-hint {\n  color: #666;\n  font-size: 12px;\n  margin-top: 3px;\n}\n\n.radio-group {\n  display: flex;\n  gap: 20px;\n  padding: 10px;\n}\n\n.radio-group label {\n  display: flex;\n  align-items: