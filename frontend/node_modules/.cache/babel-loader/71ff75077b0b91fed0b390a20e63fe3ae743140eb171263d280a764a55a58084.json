{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nimport countries from 'i18n-iso-countries';\nimport ja from 'i18n-iso-countries/langs/ja.json';\n\n// 日本語ロケール登録\ncountries.registerLocale(ja);\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        name: '',\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: '',\n      countries: []\n    };\n  },\n  created() {\n    // 国名リストを取得（日本語）\n    const countryObj = countries.getNames('ja');\n    this.countries = Object.values(countryObj).sort();\n    console.log('取得した国数:', this.countries.length);\n    console.log('最初の10カ国:', this.countries.slice(0, 10));\n    console.log('countries配列:', this.countries);\n  },\n  methods: {\n    async fetchAddress() {\n      if (this.form.postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${this.form.postalCode}`);\n          const data = await response.json();\n          if (data.results) {\n            this.form.prefecture = data.results[0].address1;\n            this.form.city = data.results[0].address2 + data.results[0].address3;\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error);\n        }\n      }\n    },\n    async handleRegister() {\n      this.errorMessage = '';\n      this.successMessage = '';\n      const favoriteFoods = this.favoriteFoodsInput.split(',').map(food => food.trim()).filter(food => food !== '');\n      try {\n        const response = await fetch('http://localhost:8081/api/users/register', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            ...this.form,\n            favoriteFoods: favoriteFoods\n          })\n        });\n        const data = await response.json();\n        if (response.ok) {\n          this.successMessage = '登録成功！ログイン画面に移動します...';\n          setTimeout(() => {\n            this.$router.push('/login');\n          }, 2000);\n        } else {\n          this.errorMessage = data.error;\n        }\n      } catch (error) {\n        this.errorMessage = '接続エラーが発生しました';\n      }\n    },\n    goToLogin() {\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["countries","ja","registerLocale","name","data","form","username","password","gender","postalCode","prefecture","city","address","phoneNumber","nationality","favoriteFoodsInput","errorMessage","successMessage","created","countryObj","getNames","Object","values","sort","console","log","length","slice","methods","fetchAddress","response","fetch","json","results","address1","address2","address3","error","handleRegister","favoriteFoods","split","map","food","trim","filter","method","headers","body","JSON","stringify","ok","setTimeout","$router","push","goToLogin"],"sources":["/Users/taharak_tabthinkmacbookpro/eclipse-workspace /tabthink-learning/frontend/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div class=\"register-container\">\n    <h1>ユーザー登録</h1>\n    <form @submit.prevent=\"handleRegister\">\n      <div class=\"form-group\">\n        <label>ユーザー名</label>\n        <input v-model=\"form.username\" type=\"text\" required />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>パスワード</label>\n        <input v-model=\"form.password\" type=\"password\" required />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>氏名</label>\n        <input v-model=\"form.name\" type=\"text\" required />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>性別</label>\n        <div class=\"radio-group\">\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"男\" /> 男</label>\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"女\" /> 女</label>\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"その他\" /> その他</label>\n        </div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>郵便番号（ハイフンなし）</label>\n        <input v-model=\"form.postalCode\" type=\"text\" @blur=\"fetchAddress\" placeholder=\"1000001\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>都道府県</label>\n        <input v-model=\"form.prefecture\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>市区町村</label>\n        <input v-model=\"form.city\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>市区町村以降</label>\n        <input v-model=\"form.address\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>電話番号</label>\n        <input v-model=\"form.phoneNumber\" type=\"tel\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>国籍</label>\n        <select v-model=\"form.nationality\" required>\n          <option value=\"\">選択してください</option>\n          <option value=\"日本\">日本</option>\n          <option value=\"アメリカ\">アメリカ</option>\n          <option value=\"中国\">中国</option>\n          <option value=\"韓国\">韓国</option>\n          <option value=\"その他\">その他</option>\n        </select>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>好きな食べ物（カンマ区切りで複数入力可）</label>\n        <input v-model=\"favoriteFoodsInput\" type=\"text\" placeholder=\"例: ラーメン,寿司,カレー\" />\n      </div>\n      \n      <div v-if=\"errorMessage\" class=\"error\">{{ errorMessage }}</div>\n      <div v-if=\"successMessage\" class=\"success\">{{ successMessage }}</div>\n      \n      <button type=\"submit\" class=\"register-btn\">登録</button>\n      <button type=\"button\" class=\"back-btn\" @click=\"goToLogin\">戻る</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport countries from 'i18n-iso-countries'\nimport ja from 'i18n-iso-countries/langs/ja.json'\n\n// 日本語ロケール登録\ncountries.registerLocale(ja)\n\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        name: '',\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: '',\n      countries: []\n    }\n  },\n  created() {\n    // 国名リストを取得（日本語）\n    const countryObj = countries.getNames('ja')\n    this.countries = Object.values(countryObj).sort()\n    console.log('取得した国数:', this.countries.length)\n    console.log('最初の10カ国:', this.countries.slice(0, 10))\n    console.log('countries配列:', this.countries)\n  },\n  methods: {\n    async fetchAddress() {\n      if (this.form.postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${this.form.postalCode}`)\n          const data = await response.json()\n          \n          if (data.results) {\n            this.form.prefecture = data.results[0].address1\n            this.form.city = data.results[0].address2 + data.results[0].address3\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error)\n        }\n      }\n    },\n    async handleRegister() {\n      this.errorMessage = ''\n      this.successMessage = ''\n      \n      const favoriteFoods = this.favoriteFoodsInput\n        .split(',')\n        .map(food => food.trim())\n        .filter(food => food !== '')\n      \n      try {\n        const response = await fetch('http://localhost:8081/api/users/register', {\n          method: 'POST',\n          headers: { 'Content-Type': 'application/json' },\n          body: JSON.stringify({\n            ...this.form,\n            favoriteFoods: favoriteFoods\n          })\n        })\n        \n        const data = await response.json()\n        \n        if (response.ok) {\n          this.successMessage = '登録成功！ログイン画面に移動します...'\n          setTimeout(() => {\n            this.$router.push('/login')\n          }, 2000)\n        } else {\n          this.errorMessage = data.error\n        }\n      } catch (error) {\n        this.errorMessage = '接続エラーが発生しました'\n      }\n    },\n    goToLogin() {\n      this.$router.push('/login')\n    }\n  }\n}\n</script>\n\n<style scoped>\n.register-container {\n  max-width: 500px;\n  margin: 50px auto;\n  padding: 30px;\n  border: 1px solid #ddd;\n  border-radius: 8px;\n}\n\nh1 {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\nlabel {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: bold;\n}\n\ninput, select {\n  width: 100%;\n  padding: 10px;\n  font-size: 14px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  box-sizing: border-box;\n}\n\n.radio-group {\n  display: flex;\n  gap: 20px;\n}\n\n.radio-group label {\n  display: flex;\n  align-items: center;\n  font-weight: normal;\n}\n\n.radio-group input {\n  width: auto;\n  margin-right: 5px;\n}\n\n.register-btn, .back-btn {\n  width: 100%;\n  padding: 12px;\n  font-size: 16px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  margin-bottom: 10px;\n}\n\n.register-btn {\n  background-color: #4CAF50;\n  color: white;\n}\n\n.back-btn {\n  background-color: #999;\n  color: white;\n}\n\n.error {\n  color: red;\n  margin-bottom: 15px;\n  text-align: center;\n}\n\n.success {\n  color: green;\n  margin-bottom: 15px;\n  text-align: center;\n}\n</style>"],"mappings":";;;;;AAgFA,OAAOA,SAAQ,MAAO,oBAAmB;AACzC,OAAOC,EAAC,MAAO,kCAAiC;;AAEhD;AACAD,SAAS,CAACE,cAAc,CAACD,EAAE;AAE3B,e