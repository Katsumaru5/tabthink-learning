{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        name: '',\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: ''\n    };\n  },\n  methods: {\n    async fetchAddress() {\n      if (this.form.postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${this.form.postalCode}`);\n          const data = await response.json();\n          if (data.results) {\n            this.form.prefecture = data.results[0].address1;\n            this.form.city = data.results[0].address2 + data.results[0].address3;\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error);\n        }\n      }\n    },\n    async handleRegister() {\n      this.errorMessage = '';\n      this.successMessage = '';\n      const favoriteFoods = this.favoriteFoodsInput.split(',').map(food => food.trim()).filter(food => food !== '');\n      try {\n        const response = await fetch('http://localhost:8081/api/users/register', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            ...this.form,\n            favoriteFoods: favoriteFoods\n          })\n        });\n        const data = await response.json();\n        if (response.ok) {\n          this.successMessage = '登録成功！ログイン画面に移動します...';\n          setTimeout(() => {\n            this.$router.push('/login');\n          }, 2000);\n        } else {\n          this.errorMessage = data.error;\n        }\n      } catch (error) {\n        this.errorMessage = '接続エラーが発生しました';\n      }\n    },\n    goToLogin() {\n      this.$router.push('/login');\n    }\n  }\n};","map":{"version":3,"names":["name","data","form","username","password","gender","postalCode","prefecture","city","address","phoneNumber","nationality","favoriteFoodsInput","errorMessage","successMessage","methods","fetchAddress","length","response","fetch","json","results","address1","address2","address3","error","console","handleRegister","favoriteFoods","split","map","food","trim","filter","method","headers","body","JSON","stringify","ok","setTimeout","$router","push","goToLogin"],"sources":["/Users/taharak_tabthinkmacbookpro/eclipse-workspace /tabthink-learning/frontend/src/views/RegisterView.vue"],"sourcesContent":["<template>\n  <div class=\"register-container\">\n    <h1>ユーザー登録</h1>\n    <form @submit.prevent=\"handleRegister\">\n      <div class=\"form-group\">\n        <label>ユーザー名</label>\n        <input v-model=\"form.username\" type=\"text\" required />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>パスワード</label>\n        <input v-model=\"form.password\" type=\"password\" required />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>氏名</label>\n        <input v-model=\"form.name\" type=\"text\" required />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>性別</label>\n        <div class=\"radio-group\">\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"男\" /> 男</label>\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"女\" /> 女</label>\n          <label><input type=\"radio\" v-model=\"form.gender\" value=\"その他\" /> その他</label>\n        </div>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>郵便番号（ハイフンなし）</label>\n        <input v-model=\"form.postalCode\" type=\"text\" @blur=\"fetchAddress\" placeholder=\"1000001\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>都道府県</label>\n        <input v-model=\"form.prefecture\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>市区町村</label>\n        <input v-model=\"form.city\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>市区町村以降</label>\n        <input v-model=\"form.address\" type=\"text\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>電話番号</label>\n        <input v-model=\"form.phoneNumber\" type=\"tel\" />\n      </div>\n      \n      <div class=\"form-group\">\n        <label>国籍</label>\n        <select v-model=\"form.nationality\" required>\n          <option value=\"\">選択してください</option>\n          <option value=\"日本\">日本</option>\n          <option value=\"アメリカ\">アメリカ</option>\n          <option value=\"中国\">中国</option>\n          <option value=\"韓国\">韓国</option>\n          <option value=\"その他\">その他</option>\n        </select>\n      </div>\n      \n      <div class=\"form-group\">\n        <label>好きな食べ物（カンマ区切りで複数入力可）</label>\n        <input v-model=\"favoriteFoodsInput\" type=\"text\" placeholder=\"例: ラーメン,寿司,カレー\" />\n      </div>\n      \n      <div v-if=\"errorMessage\" class=\"error\">{{ errorMessage }}</div>\n      <div v-if=\"successMessage\" class=\"success\">{{ successMessage }}</div>\n      \n      <button type=\"submit\" class=\"register-btn\">登録</button>\n      <button type=\"button\" class=\"back-btn\" @click=\"goToLogin\">戻る</button>\n    </form>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      form: {\n        username: '',\n        password: '',\n        name: '',\n        gender: '',\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      errorMessage: '',\n      successMessage: ''\n    }\n  },\n  methods: {\n    async fetchAddress() {\n      if (this.form.postalCode.length === 7) {\n        try {\n          const response = await fetch(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${this.form.postalCode}`)\n          const data = await response.json()\n          \n          if (data.results) {\n            this.form.prefecture = data.results[0].address1\n            this.form.city = data.results[0].address2 + data.results[0].address3\n          }\n        } catch (error) {\n          console.error('住所取得エラー:', error)\n        }\n      }\n    },\n    async handleRegister() {\n      this.errorMessage = ''\n      this.successMessage = ''\n      \n      const favoriteFoods = this.favoriteFoodsInput\n        .split(',')\n        .map(food => food.trim())\n        .filter(food => food !== '')\n      \n      try {\n        const response = await fetch('http://localhost:8081/api/users/register', {\n          method: 'POST',\n          headers: { 'Content-Type': 'application/json' },\n          body: JSON.stringify({\n            ...this.form,\n            favoriteFoods: favoriteFoods\n          })\n        })\n        \n        const data = await response.json()\n        \n        if (response.ok) {\n          this.successMessage = '登録成功！ログイン画面に移動します...'\n          setTimeout(() => {\n            this.$router.push('/login')\n          }, 2000)\n        } else {\n          this.errorMessage = data.error\n        }\n      } catch (error) {\n        this.errorMessage = '接続エラーが発生しました'\n      }\n    },\n    goToLogin() {\n      this.$router.push('/login')\n    }\n  }\n}\n</script>\n\n<style scoped>\n.register-container {\n  max-width: 500px;\n  margin: 50px auto;\n  padding: 30px;\n  border: 1px solid #ddd;\n  border-radius: 8px;\n}\n\nh1 {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.form-group {\n  margin-bottom: 20px;\n}\n\nlabel {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: bold;\n}\n\ninput, select {\n  width: 100%;\n  padding: 10px;\n  font-size: 14px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  box-sizing: border-box;\n}\n\n.radio-group {\n  display: flex;\n  gap: 20px;\n}\n\n.radio-group label {\n  display: flex;\n  align-items: center;\n  font-weight: normal;\n}\n\n.radio-group input {\n  width: auto;\n  margin-right: 5px;\n}\n\n.register-btn, .back-btn {\n  width: 100%;\n  padding: 12px;\n  font-size: 16px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  margin-bottom: 10px;\n}\n\n.register-btn {\n  background-color: #4CAF50;\n  color: white;\n}\n\n.back-btn {\n  background-color: #999;\n  color: white;\n}\n\n.error {\n  color: red;\n  margin-bottom: 15px;\n  text-align: center;\n}\n\n.success {\n  color: green;\n  margin-bottom: 15px;\n  text-align: center;\n}\n</style>"],"mappings":";;;;;AAgFA,eAAe;EACbA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZJ,IAAI,EAAE,EAAE;QACRK,MAAM,EAAE,EAAE;QACVC,UAAU,EAAE,EAAE;QACdC,UAAU,EAAE,EAAE;QACdC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,WAAW,EAAE,EAAE;QACfC,WAAW,EAAE;MACf,CAAC;MACDC,kBAAkB,EAAE,EAAE;MACtBC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE;IAClB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI,IAAI,CAACd,IAAI,CAACI,UAAU,CAACW,MAAK,KAAM,CAAC,EAAE;QACrC,IAAI;UACF,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,oDAAoD,IAAI,CAACjB,IAAI,CAACI,UAAU,EAAE;UACvG,MAAML,IAAG,GAAI,MAAMiB,QAAQ,CAACE,IAAI,CAAC;UAEjC,IAAInB,IAAI,CAACoB,OAAO,EAAE;YAChB,IAAI,CAACnB,IAAI,CAACK,UAAS,GAAIN,IAAI,CAACoB,OAAO,CAAC,CAAC,CAAC,CAACC,QAAO;YAC9C,IAAI,CAACpB,IAAI,CAACM,IAAG,GAAIP,IAAI,CAACoB,OAAO,CAAC,CAAC,CAAC,CAACE,QAAO,GAAItB,IAAI,CAACoB,OAAO,CAAC,CAAC,CAAC,CAACG,QAAO;UACrE;QACF,EAAE,OAAOC,KAAK,EAAE;UACdC,OAAO,CAACD,KAAK,CAAC,UAAU,EAAEA,KAAK;QACjC;MACF;IACF,CAAC;IACD,MAAME,cAAcA,CAAA,EAAG;MACrB,IAAI,CAACd,YAAW,GAAI,EAAC;MACrB,IAAI,CAACC,cAAa,GAAI,EAAC;MAEvB,MAAMc,aAAY,GAAI,IAAI,CAAChB,kBAAiB,CACzCiB,KAAK,CAAC,GAAG,EACTC,GAAG,CAACC,IAAG,IAAKA,IAAI,CAAC