{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport axios from 'axios';\nexport default {\n  name: 'UserEditView',\n  data() {\n    return {\n      userId: null,\n      formData: {\n        username: '',\n        name: '',\n        gender: '',\n        age: null,\n        postalCode: '',\n        prefecture: '',\n        city: '',\n        address: '',\n        phoneNumber: '',\n        nationality: ''\n      },\n      favoriteFoodsInput: '',\n      fieldErrors: {},\n      postalCodeNotFound: false\n    };\n  },\n  computed: {\n    hasErrors() {\n      return Object.keys(this.fieldErrors).length > 0 || this.postalCodeNotFound;\n    }\n  },\n  async mounted() {\n    this.userId = this.$route.params.id;\n    await this.loadUser();\n  },\n  methods: {\n    // „É¶„Éº„Ç∂„ÉºÊÉÖÂ†±Ë™≠„ÅøËæº„Åø\n    async loadUser() {\n      try {\n        const response = await axios.get(`http://localhost:8081/api/users/${this.userId}`);\n        const user = response.data;\n        this.formData = {\n          username: user.username,\n          name: user.name,\n          gender: user.gender === 'Áî∑' ? 'Áî∑ÊÄß' : user.gender === 'Â•≥' ? 'Â•≥ÊÄß' : user.gender,\n          age: user.age,\n          postalCode: user.postalCode ? user.postalCode.replace(/-/g, '') : '',\n          prefecture: user.prefecture,\n          city: user.city,\n          address: user.address,\n          phoneNumber: user.phoneNumber ? user.phoneNumber.replace(/-/g, '') : '',\n          nationality: user.nationality\n        };\n        this.favoriteFoodsInput = user.favoriteFoods ? user.favoriteFoods.join(',') : '';\n      } catch (error) {\n        console.error('„É¶„Éº„Ç∂„ÉºÊÉÖÂ†±„ÅÆÂèñÂæó„Å´Â§±Êïó„Åó„Åæ„Åó„Åü:', error);\n        alert('„É¶„Éº„Ç∂„ÉºÊÉÖÂ†±„ÅÆÂèñÂæó„Å´Â§±Êïó„Åó„Åæ„Åó„Åü');\n      }\n    },\n    // „Éï„Ç£„Éº„É´„Éâ„Ç®„É©„Éº„Çí„ÇØ„É™„Ç¢\n    clearFieldError(fieldName) {\n      if (this.fieldErrors[fieldName]) {\n        delete this.fieldErrors[fieldName];\n      }\n    },\n    // ÈÉµ‰æøÁï™Âè∑„Éï„Ç©„Éº„Éû„ÉÉ„ÉàÔºà„Éè„Ç§„Éï„É≥„Å™„ÅóÔºâ\n    formatPostalCodeNoHyphen() {\n      this.formData.postalCode = this.formData.postalCode.replace(/[^0-9]/g, '').substring(0, 7);\n      this.clearFieldError('postalCode');\n      this.postalCodeNotFound = false;\n    },\n    // ÈõªË©±Áï™Âè∑„Éï„Ç©„Éº„Éû„ÉÉ„ÉàÔºà„Éè„Ç§„Éï„É≥„Å™„ÅóÔºâ\n    formatPhoneNumberNoHyphen() {\n      this.formData.phoneNumber = this.formData.phoneNumber.replace(/[^0-9]/g, '').substring(0, 11);\n      this.clearFieldError('phoneNumber');\n    },\n    // ‰ΩèÊâÄËá™ÂãïÂÖ•Âäõ\n    async fetchAddress() {\n      if (this.formData.postalCode.length === 7) {\n        try {\n          const response = await axios.get(`https://zipcloud.ibsnet.co.jp/api/search?zipcode=${this.formData.postalCode}`);\n          if (response.data.results) {\n            const address = response.data.results[0];\n            this.formData.prefecture = address.address1;\n            this.formData.city = address.address2;\n            this.postalCodeNotFound = false;\n          } else {\n            this.formData.prefecture = '';\n            this.formData.city = '';\n            this.postalCodeNotFound = true;\n          }\n        } catch (error) {\n          console.error('‰ΩèÊâÄÂèñÂæó„Ç®„É©„Éº:', error);\n        }\n      }\n    },\n    // Êõ¥Êñ∞Âá¶ÁêÜ\n    async handleUpdate() {\n      // ÈÉµ‰æøÁï™Âè∑„ÅÆÂ≠òÂú®„ÉÅ„Çß„ÉÉ„ÇØ\n      if (this.postalCodeNotFound) {\n        this.scrollToTop();\n        return;\n      }\n\n      // Â•Ω„Åç„Å™È£ü„ÅπÁâ©„ÇíÈÖçÂàó„Å´Â§âÊèõ\n      const favoriteFoodsArray = this.favoriteFoodsInput ? this.favoriteFoodsInput.split(',').map(item => item.trim()).filter(item => item) : [];\n      const requestData = {\n        name: this.formData.name,\n        gender: this.formData.gender,\n        age: this.formData.age,\n        postalCode: this.formData.postalCode,\n        prefecture: this.formData.prefecture,\n        city: this.formData.city,\n        address: this.formData.address,\n        phoneNumber: this.formData.phoneNumber,\n        nationality: this.formData.nationality,\n        favoriteFoods: favoriteFoodsArray\n      };\n      console.log('üì§ ÈÄÅ‰ø°„Éá„Éº„Çø:', requestData);\n      try {\n        const response = await axios.put(`http://localhost:8081/api/users/${this.userId}`, requestData);\n        console.log('‚úÖ „Çµ„Éº„Éê„ÉºÂøúÁ≠î:', response.data);\n        if (response.data.success) {\n          alert('Êõ¥Êñ∞„ÅåÂÆå‰∫Ü„Åó„Åæ„Åó„Åü');\n          this.$router.push('/users');\n        }\n      } catch (error) {\n        console.error('‚ùå Êõ¥Êñ∞„Ç®„É©„Éº:', error);\n        if (error.response && error.response.data) {\n          const data = error.response.data;\n          if (data.errors) {\n            // „Éê„É™„Éá„Éº„Ç∑„Éß„É≥„Ç®„É©„Éº„ÇíË®≠ÂÆö\n            this.fieldErrors = data.errors;\n\n            // „Éö„Éº„Ç∏‰∏äÈÉ®„Å´„Çπ„ÇØ„É≠„Éº„É´\n            this.scrollToTop();\n          } else if (data.message) {\n            alert(data.message);\n          }\n        } else {\n          alert('Êé•Á∂ö„Ç®„É©„Éº„ÅåÁô∫Áîü„Åó„Åæ„Åó„Åü');\n        }\n      }\n    },\n    // „Éö„Éº„Ç∏‰∏äÈÉ®„Å´„Çπ„ÇØ„É≠„Éº„É´\n    scrollToTop() {\n      this.$nextTick(() => {\n        if (this.$refs.errorSummary) {\n          this.$refs.errorSummary.scrollIntoView({\n            behavior: 'smooth',\n            block: 'start'\n          });\n        } else {\n          window.scrollTo({\n            top: 0,\n            behavior: 'smooth'\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","userId","formData","username","gender","age","postalCode","prefecture","city","address","phoneNumber","nationality","favoriteFoodsInput","fieldErrors","postalCodeNotFound","computed","hasErrors","Object","keys","length","mounted","$route","params","id","loadUser","methods","response","get","user","replace","favoriteFoods","join","error","console","alert","clearFieldError","fieldName","formatPostalCodeNoHyphen","substring","formatPhoneNumberNoHyphen","fetchAddress","results","address1","address2","handleUpdate","scrollToTop","favoriteFoodsArray","split","map","item","trim","filter","requestData","log","put","success","$router","push","errors","message","$nextTick","$refs","errorSummary","scrollIntoView","behavior","block","window","scrollTo","top"],"sources":["/Users/taharak_tabthinkmacbookpro/eclipse-workspace /tabthink-learning/frontend/src/views/UserEditView.vue"],"sourcesContent":["<template>\n  <div class=\"edit-container\">\n    <h2>„É¶„Éº„Ç∂„ÉºÊÉÖÂ†±Á∑®ÈõÜ</h2>\n\n    <!-- „Ç®„É©„Éº„Çµ„Éû„É™„ÉºÔºà‰∏ÄÁï™‰∏äÔºâ -->\n    <div v-if=\"hasErrors\" class=\"error-summary\" ref=\"errorSummary\">\n      ‚ö†Ô∏è ÂÖ•ÂäõÂÜÖÂÆπ„Å´Ë™§„Çä„Åå„ÅÇ„Çä„Åæ„Åô\n    </div>\n\n    <form @submit.prevent=\"handleUpdate\" class=\"edit-form\">\n      <!-- „É¶„Éº„Ç∂„ÉºÂêçÔºàÁ∑®ÈõÜ‰∏çÂèØÔºâ -->\n      <div class=\"form-group\">\n        <label>„É¶„Éº„Ç∂„ÉºÂêç</label>\n        <input type=\"text\" v-model=\"formData.username\" disabled />\n      </div>\n\n      <!-- Ê∞èÂêç -->\n      <div class=\"form-group\">\n        <label>Ê∞èÂêç <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.name\"\n          :class=\"{ 'error-input': fieldErrors.name }\"\n          @input=\"clearFieldError('name')\"\n          placeholder=\"‰æã: Â±±Áî∞Â§™ÈÉé\"\n        />\n        <div v-if=\"fieldErrors.name\" class=\"error-message\">\n          {{ fieldErrors.name }}\n        </div>\n      </div>\n\n      <!-- ÊÄßÂà• -->\n      <div class=\"form-group\">\n        <label>ÊÄßÂà• <span class=\"required\">*</span></label>\n        <div :class=\"{ 'error-input': fieldErrors.gender }\">\n          <label class=\"radio-label\">\n            <input\n              type=\"radio\"\n              v-model=\"formData.gender\"\n              value=\"Áî∑ÊÄß\"\n              @change=\"clearFieldError('gender')\"\n            />\n            Áî∑ÊÄß\n          </label>\n          <label class=\"radio-label\">\n            <input\n              type=\"radio\"\n              v-model=\"formData.gender\"\n              value=\"Â•≥ÊÄß\"\n              @change=\"clearFieldError('gender')\"\n            />\n            Â•≥ÊÄß\n          </label>\n          <label class=\"radio-label\">\n            <input\n              type=\"radio\"\n              v-model=\"formData.gender\"\n              value=\"„Åù„ÅÆ‰ªñ\"\n              @change=\"clearFieldError('gender')\"\n            />\n            „Åù„ÅÆ‰ªñ\n          </label>\n        </div>\n        <div v-if=\"fieldErrors.gender\" class=\"error-message\">\n          {{ fieldErrors.gender }}\n        </div>\n      </div>\n\n      <!-- Âπ¥ÈΩ¢ -->\n      <div class=\"form-group\">\n        <label>Âπ¥ÈΩ¢ <span class=\"required\">*</span></label>\n        <input\n          type=\"number\"\n          v-model.number=\"formData.age\"\n          :class=\"{ 'error-input': fieldErrors.age }\"\n          @input=\"clearFieldError('age')\"\n          min=\"0\"\n          max=\"150\"\n          placeholder=\"‰æã: 25\"\n        />\n        <div v-if=\"fieldErrors.age\" class=\"error-message\">\n          {{ fieldErrors.age }}\n        </div>\n      </div>\n\n      <!-- ÈÉµ‰æøÁï™Âè∑ -->\n      <div class=\"form-group\">\n        <label>ÈÉµ‰æøÁï™Âè∑ <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.postalCode\"\n          :class=\"{ 'error-input': fieldErrors.postalCode || postalCodeNotFound }\"\n          @input=\"formatPostalCodeNoHyphen\"\n          @blur=\"fetchAddress\"\n          maxlength=\"7\"\n          placeholder=\"‰æã: 1000001Ôºà„Éè„Ç§„Éï„É≥„Å™„Åó7Ê°ÅÔºâ\"\n        />\n        <div v-if=\"fieldErrors.postalCode\" class=\"error-message\">\n          {{ fieldErrors.postalCode }}\n        </div>\n        <div v-if=\"postalCodeNotFound\" class=\"error-message\">\n          „Åì„ÅÆÈÉµ‰æøÁï™Âè∑„ÅØÂ≠òÂú®„Åó„Åæ„Åõ„Çì\n        </div>\n      </div>\n\n      <!-- ÈÉΩÈÅìÂ∫úÁúå -->\n      <div class=\"form-group\">\n        <label>ÈÉΩÈÅìÂ∫úÁúå <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.prefecture\"\n          :class=\"{ 'error-input': fieldErrors.prefecture }\"\n          @input=\"clearFieldError('prefecture')\"\n          placeholder=\"‰æã: Êù±‰∫¨ÈÉΩ\"\n        />\n        <div v-if=\"fieldErrors.prefecture\" class=\"error-message\">\n          {{ fieldErrors.prefecture }}\n        </div>\n      </div>\n\n      <!-- Â∏ÇÂå∫Áî∫Êùë -->\n      <div class=\"form-group\">\n        <label>Â∏ÇÂå∫Áî∫Êùë <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.city\"\n          :class=\"{ 'error-input': fieldErrors.city }\"\n          @input=\"clearFieldError('city')\"\n          placeholder=\"‰æã: ÂçÉ‰ª£Áî∞Âå∫\"\n        />\n        <div v-if=\"fieldErrors.city\" class=\"error-message\">\n          {{ fieldErrors.city }}\n        </div>\n      </div>\n\n      <!-- ‰ΩèÊâÄ -->\n      <div class=\"form-group\">\n        <label>‰ΩèÊâÄ <span class=\"required\">*</span></label>\n        <input\n          type=\"text\"\n          v-model=\"formData.address\"\n          :class=\"{ 'error-input': fieldErrors.address }\"\n          @input=\"clearFieldError('address')\"\n          placeholder=\"‰æã: ÂçÉ‰ª£Áî∞1-1-1\"\n        />\n        <div v-if=\"fieldErrors.address\" class=\"error-message\">\n          {{ fieldErrors.address }}\n        </div>\n      </div>\n\n      <!-- ÈõªË©±Áï™Âè∑ -->\n      <div class=\"form-group\">\n        <label>ÈõªË©±Áï™Âè∑ <span class=\"required\">*</span></label>\n        <input\n          type=\"tel\"\n          v-model=\"formData.phoneNumber\"\n          :class=\"{ 'error-input': fieldErrors.phoneNumber }\"\n          @input=\"formatPhoneNumberNoHyphen\"\n          maxlength=\"11\"\n          placeholder=\"‰æã: 09012345678Ôºà„Éè„Ç§„Éï„É≥„Å™„Åó10„Äú11Ê°ÅÔºâ\"\n        />\n        <div v-if=\"fieldErrors.phoneNumber\" class=\"error-message\">\n          {{ fieldErrors.phoneNumber }}\n        </div>\n      </div>\n\n      <!-- ÂõΩÁ±ç -->\n      <div class=\"form-group\">\n        <label>ÂõΩÁ±ç <span class=\"required\">*</span></label>\n        <select\n          v-model=\"formData.nationality\"\n          :class=\"{ 'error-input': fieldErrors.nationality }\"\n          @change=\"clearFieldError('nationality')\"\n        >\n          <option value=\"\">ÈÅ∏Êäû„Åó„Å¶„Åè„Å†„Åï„ÅÑ</option>\n          <option value=\"Êó•Êú¨\">Êó•Êú¨</option>\n          <option value=\"„Ç¢„É°„É™„Ç´\">„Ç¢„É°„É™„Ç´</option>\n          <option value=\"‰∏≠ÂõΩ\">‰∏≠ÂõΩ</option>\n          <option value=\"ÈüìÂõΩ\">ÈüìÂõΩ</option>\n          <option value=\"„Åù„ÅÆ‰ªñ\">„Åù„ÅÆ‰ªñ</option>\n        </select>\n        <div v-if=\"fieldErrors.nationality\" class=\"error-message\">\n          {{ fieldErrors.nationality }}\n        </div>\n      </div>\n\n      <!-- Â•Ω„Åç„Å™È£ü„ÅπÁâ© -->\n      <div class=\"form-group\">\n        <label>Â•Ω„Åç„Å™È£ü„ÅπÁâ©</label>\n        <input\n          type=\"text\"\n          v-model=\"favoriteFoodsInput\"\n          :class=\"{ 'error-input': fieldErrors.favoriteFoods }\"\n          @input=\"clearFieldError('favoriteFoods')\"\n          placeholder=\"‰æã: ÂØøÂè∏,„É©„Éº„É°„É≥,„Ç´„É¨„ÉºÔºà„Ç´„É≥„ÉûÂå∫Âàá„ÇäÔºâ\"\n        />\n        <div v-if=\"fieldErrors.favoriteFoods\" class=\"error-message\">\n          {{ fieldErrors.favoriteFoods }}\n        </div>\n      </div>\n\n      <!-- „Éú„Çø„É≥ -->\n      <div class=\"button-group\">\n        <button type=\"submit\" class=\"btn-primary\">Êõ¥Êñ∞</button>\n        <button type=\"button\" @click=\"$router.push('/users')\" class=\"btn-secondary\">\n          „Ç≠„É£„É≥„Çª„É´\n        </button>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nimport 